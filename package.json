{
  "name": "react-ts-seed",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "dependencies": {
    "@material-ui/core": "~4.9.12",
    "@material-ui/icons": "~4.9.1",
    "@material-ui/lab": "~4.0.0-alpha.49",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.2",
    "graphql": "~15.0.0-rc.1",
    "graphql.macro": "~1.4.2",
    "lodash": "~4.17.15",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-router-dom": "~5.1.2",
    "react-scripts": "~3.4.1",
    "react-spring": "^9.0.0-beta.34",
    "react-use-gesture": "^7.0.15",
    "styled-components": "~5.1.0",
    "styled-system": "~5.1.5",
    "styled-tools": "~1.7.1",
    "typeface-montserrat-alternates": "~0.0.72",
    "typeface-open-sans": "~0.0.75",
    "typeface-open-sans-condensed": "~0.0.72",
    "typeface-playfair-display": "~1.1.4",
    "typescript": "~3.8.3"
  },
  "scripts": {
    "commit": "npx git-cz",
    "start": "npm run lint && react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test -w 1",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/prettier --fix --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "storybook": "rimraf ./node_modules/.cache/storybook && cross-env NODE_OPTIONS=--max_old_space_size=8192 && start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test-no-watch": "react-scripts test --watchAll=false",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "eslint": "eslint src/**/*.{ts,tsx} --ext .js,.jsx,.ts,.tsx --fix",
    "generate": "plop --plopfile generators/index.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ]
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test-no-watch",
      "pre-push": "lint-staged && npm run lint && npm run test-no-watch",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/runtime": "^7.9.2",
    "@commitlint/cli": "~8.3.5",
    "@commitlint/config-conventional": "~8.3.4",
    "@storybook/addon-actions": "~5.3.18",
    "@storybook/addon-docs": "~5.3.18",
    "@storybook/addon-info": "~5.3.18",
    "@storybook/addon-links": "~5.3.18",
    "@storybook/addons": "~5.3.18",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/preset-typescript": "~3.0.0",
    "@storybook/react": "~5.3.18",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^10.0.1",
    "@types/css-mediaquery": "^0.1.0",
    "@types/enzyme": "~3.10.5",
    "@types/jest": "~25.2.1",
    "@types/lodash": "~4.14.149",
    "@types/node": "~13.11.1",
    "@types/react": "~16.9.34",
    "@types/react-dom": "~16.9.7",
    "@types/react-test-renderer": "~16.9.2",
    "@types/styled-components": "~5.1.0",
    "@types/styled-system": "~5.1.9",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "~8.1.0",
    "babel-plugin-import": "~1.13.0",
    "babel-plugin-macros": "~2.8.0",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-styled-components": "~1.10.7",
    "babel-preset-react-app": "^9.1.2",
    "codecov": "^4.0.0-0",
    "cross-env": "~7.0.2",
    "css-loader": "~3.5.3",
    "css-mediaquery": "^0.1.2",
    "customize-cra": "^1.0.0-alpha.0",
    "enzyme-to-json": "~3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "faker": "^4.1.0",
    "git-cz": "^4.3.1",
    "husky": "~4.2.5",
    "jest-styled-components": "~7.0.2",
    "lint-staged": "~10.2.0",
    "node-plop": "^0.25.0",
    "node-sass": "~4.14.0",
    "nyc": "^15.0.1",
    "plop": "^2.6.0",
    "prettier": "^2.0.5",
    "react-app-rewired": "^2.1.5",
    "react-docgen-typescript-loader": "~3.7.2",
    "react-is": "~16.13.1",
    "react-test-renderer": "~16.13.1",
    "require-context.macro": "~1.2.2",
    "rimraf": "~3.0.2",
    "snapshot-diff": "^0.7.0",
    "source-map-explorer": "~2.4.2",
    "style-loader": "~1.2.1",
    "stylelint": "~13.3.3",
    "stylelint-config-recommended": "~3.0.0",
    "stylelint-config-styled-components": "~0.1.1",
    "stylelint-order": "^4.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-loader": "^8.0.11",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.stories.tsx",
      "!src/**/index.ts",
      "!src/**/*.types.ts",
      "!src/**/*.mock.ts",
      "!src/*.ts",
      "!src/index.tsx",
      "!src/utils/type.ts",
      "!src/utils/loadable.tsx",
      "!generators",
      "!src/**/Loadable.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
