{
  "name": "react-ts-seed",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "~4.8.3",
    "@material-ui/icons": "~4.5.1",
    "@material-ui/lab": "~4.0.0-alpha.33",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "~1.15.2",
    "graphql": "~14.5.8",
    "graphql.macro": "~1.4.2",
    "lodash": "~4.17.15",
    "react": "~16.12.0",
    "react-dom": "~16.12.0",
    "react-router-dom": "~5.1.2",
    "react-scripts": "~3.3.0",
    "react-spring": "^9.0.0-beta.34",
    "styled-components": "~4.4.1",
    "styled-system": "~5.1.4",
    "styled-tools": "~1.7.1",
    "typeface-montserrat-alternates": "~0.0.72",
    "typeface-open-sans": "~0.0.75",
    "typeface-open-sans-condensed": "~0.0.72",
    "typeface-playfair-display": "~0.0.72",
    "typescript": "~3.7.4"
  },
  "scripts": {
    "commit": "npx git-cz",
    "start": "npm run lint && react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/prettier --fix --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "storybook": "rimraf ./node_modules/.cache/storybook && start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test-no-watch": "react-scripts test --watchAll=false",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "eslint": "eslint src/**/*.{ts,tsx} --ext .js,.jsx,.ts,.tsx --fix",
    "generate": "plop --plopfile generators/index.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ]
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test-no-watch",
      "pre-push": "lint-staged && npm run lint && npm run test-no-watch",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "~8.2.0",
    "@commitlint/config-conventional": "~8.2.0",
    "@storybook/addon-actions": "~5.3.0-alpha.42",
    "@storybook/addon-docs": "~5.2.8",
    "@storybook/addon-info": "~5.2.8",
    "@storybook/addon-links": "~5.3.0-alpha.42",
    "@storybook/addons": "~5.3.0-alpha.42",
    "@storybook/preset-create-react-app": "~1.5.0",
    "@storybook/preset-typescript": "~1.2.0",
    "@storybook/react": "~5.3.0-alpha.42",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^8.0.2",
    "@types/enzyme": "~3.10.4",
    "@types/jest": "~24.0.25",
    "@types/lodash": "~4.14.149",
    "@types/node": "~13.1.2",
    "@types/react": "~16.9.17",
    "@types/react-dom": "~16.9.4",
    "@types/react-test-renderer": "~16.9.1",
    "@types/styled-components": "~4.4.1",
    "@types/styled-system": "~5.1.4",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "~8.0.6",
    "babel-plugin-import": "~1.13.0",
    "babel-plugin-macros": "~2.8.0",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-styled-components": "~1.10.6",
    "codecov": "^3.6.1",
    "cross-env": "~6.0.3",
    "css-loader": "~3.4.0",
    "customize-cra": "^0.9.1",
    "enzyme-to-json": "~3.4.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "faker": "^4.1.0",
    "git-cz": "^3.3.0",
    "husky": "~3.1.0",
    "jest-styled-components": "~6.3.4",
    "lint-staged": "~9.5.0",
    "node-plop": "^0.23.0",
    "node-sass": "~4.13.0",
    "nyc": "^15.0.0",
    "plop": "^2.5.3",
    "prettier": "^1.19.1",
    "react-app-rewired": "^2.1.5",
    "react-docgen-typescript-loader": "~3.6.0",
    "react-is": "~16.12.0",
    "react-test-renderer": "~16.12.0",
    "require-context.macro": "~1.2.2",
    "rimraf": "~3.0.0",
    "snapshot-diff": "^0.6.1",
    "source-map-explorer": "~2.2.0",
    "style-loader": "~1.1.2",
    "stylelint": "~12.0.1",
    "stylelint-config-recommended": "~3.0.0",
    "stylelint-config-styled-components": "~0.1.1",
    "stylelint-order": "^3.1.1",
    "stylelint-processor-styled-components": "^1.9.0",
    "ts-loader": "^6.2.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.1.0",
    "tslint-react": "^4.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.stories.tsx",
      "!src/**/index.ts",
      "!src/**/*.types.ts",
      "!src/*.ts",
      "!src/index.tsx",
      "!src/utils/type.ts",
      "!src/utils/loadable.tsx",
      "!generators",
      "!src/**/Loadable.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
