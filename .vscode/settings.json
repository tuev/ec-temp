{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    { "language": "typescript", "autoFix": true },
    { "language": "typescriptreact", "autoFix": true }
  ],
  "workbench.colorCustomizations": {
    "statusBar.background": "#3c856c",
    "statusBar.noFolderBackground": "#9fb150",
    "statusBar.debuggingBackground": "#aa2d2d",
    "activityBar.background": "#0e3316",
    "sideBarSectionHeader.background": "#144127"
  },
  "todohighlight.isEnable": true,
  "todohighlight.keywords": [
    "DEBUG:",
    "REVIEW:",
    {
      "text": "NOTE:",
      "color": "#ff0000",
      "backgroundColor": "yellow",
      "overviewRulerColor": "grey"
    },
    {
      "text": "HACK:",
      "color": "#000",
      "isWholeLine": false
    },
    {
      "text": "TODO:",
      "color": "red",
      "border": "1px solid red",
      "borderRadius": "2px",
      "backgroundColor": "rgba(0,0,0,.2)"
    }
  ]
}
