{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "defaultSeverity": "warning",
  "rules": {
    "no-implicit-dependencies": [
      true,
      [
        "components",
        "containers",
        "theme",
        "@storybook",
        "utils",
        "@testing-library/react"
      ]
    ],
    "no-submodule-imports": false,
    "ordered-imports": false,
    "no-unexpected-multiline": 0
  }
}
